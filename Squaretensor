import tensorflow as tf

tensor = tf.constant([[1.0, 4.0], [9.0, 16.0]])
squared = tf.square(tensor)             
sqrted = tf.sqrt(tensor)                 
exponential = tf.exp(tensor)            
print("Original Tensor:\n", tensor.numpy())
print("Squared:\n", squared.numpy())
print("Square Root:\n", sqrted.numpy())
print("Exponential:\n", exponential.numpy())
